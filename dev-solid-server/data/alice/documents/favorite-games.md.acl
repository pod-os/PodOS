@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

<#public>
    a acl:Authorization;
    acl:agentClass foaf:Agent;
    acl:accessTo <./favorite-games.md>;
    acl:mode acl:Read.

<#owner>
    a acl:Authorization;
    acl:agent </alice/profile/card#me>;
    acl:accessTo <./favorite-games.md>;
    acl:mode
            acl:Read, acl:Write, acl:Control.
